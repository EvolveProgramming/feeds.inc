<!--Main layout-->
<div class="container">
    <div class="row">
        <!--Main column-->
        <div class="col-lg-8">
            <p id="notice"><%= notice %></p>
            <h1>Feeds</h1>
            <table>
                <thead>
                    <tr>
                        <th>Name</th>
                        <th>Url</th>
                        <th>Description</th>
                        <th colspan="3"></th>
                    </tr>
                </thead>
                <tbody>
                    <% @feeds.each do |feed| %>
                    <tr>
                        <td><%= feed.name %></td>
                        <td><%= feed.url %></td>
                        <td><%= feed.description %></td>
                        <td><%= link_to 'Show', feed %></td>
                        <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
                        <td><%= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                    </tr>
                    <% end %>
                </tbody>
            </table>
            <br>
            <%= link_to 'New Feed', new_feed_path %>
        </div>
    </div>
</div>


<div class="col-lg-8">
      <div class="well">
        <h4> Categories</h4>
        <div class="row">
          <a class="list-group-item" href="/posts?id=2">The Write to express</a>
          <a class="list-group-item" href="/posts?id=3">Poem Lookalikes</a>
          <a class="list-group-item" href="/posts?id=4">News</a>
          <!-- /.col-lg-6 -->
        </div>
        <!-- /.row -->
      </div>
    </div>